      <routine dataAccess="CONTAINS SQL" deterministic="false" name="insert_guest_user()" securityType="DEFINER" type="PROCEDURE">
         <comment/>
         <definition language="SQL"><![CDATA[BEGIN
	DECLARE user_count INT;
    
    -- Check if the "Guest" user exists
    SELECT COUNT(*) INTO user_count FROM Users WHERE UserName ='Guest';
    
    -- If the "Guest" user does not exist, insert iterate
    IF user_count = 0 THEN
		INSERT INTO Users (UserName, EmailAddress, Password)
        VALUE ('Guest', 'guest@guest.com', 'password');
	END IF;
END]]></definition>
